<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">
    <header>
        <%- include('../partials/header'); %>
    </header>
    <main>
       <div class="jombotron">
            <h2>Variable</h2>
            <p>
                <%= tagline %>
            </p>
            <ul>
                <% pets.forEach((pet) => { %>
                    <li>
                        <strong>
                            <%= pet.name%>
                        </strong> representa a <%= pet.organization %>, nacio <%= pet.birth_year %>
                    </li>
                <% }); %>
            </ul>
       </div> 
    </main>
    <Footer>
        <%- include('../partials/footer'); %>
    </Footer>
</body>
</html>